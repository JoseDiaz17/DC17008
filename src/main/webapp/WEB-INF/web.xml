<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>ServeltAgregar</servlet-name>
        <servlet-class>com.progra2.servlets.ServeltAgregar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServeltEliminar</servlet-name>
        <servlet-class>com.progra2.servlets.ServeltEliminar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServeltAgregar</servlet-name>
        <url-pattern>/ServeltAgregar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServeltEliminar</servlet-name>
        <url-pattern>/ServeltEliminar</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
